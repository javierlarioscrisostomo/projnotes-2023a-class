{
    "presets": [
        [
            "minify",{
                "builtIns": false,
                "evaluate" : false,
                "mangle" : false
            }
        ],
        [
            "@babel/preset-env",{
                //los targets son las plataformas 
                "targets" : {"node" : "current"}
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",{
                "root": ["./"],
                "alias":{
                    "@server": "./SERVER"
                }
            }
        ]
    ]
}